<div class = "header">
    <h2>Alliances</h2>
</div>

<div class = "new_alliance">
		    <a class = "new_link">
		    <i class=" material-icons md-12">keyboard_arrow_down</i>
		    <%="Add New Alliance"%></a> 

		    <div class = "org-form">
		       <% @alliance = Alliance.new %>
			<!-- now the form template itself -->
			  <%= simple_form_for @alliance do |f| %>
			        <%= f.input :name,  placeholder: 'New Alliance',input_html: {class: "input"}, wrapper_html: {class: 'wrapper'} %>
			        <%= f.button :submit %>
			  <% end %>
		    </div>
</div>

<%= render :partial => "index_details"%>


